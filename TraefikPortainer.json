{
    "version": "2",
    "templates": [
        {
            "type": 1,
            "title": "Gluetun",
            "name": "gluetun",
            "platform": "linux",
            "image": "qmcgaw/gluetun",
            "logo": "https://raw.githubusercontent.com/thelowtech/HomeLab/master/Images/gluetun.png",
            "description": "Lightweight swiss-knife-like VPN client to tunnel to Cyberghost, FastestVPN, HideMyAss, Mullvad, NordVPN, Privado, Private Internet Access, PrivateVPN, ProtonVPN, PureVPN, Surfshark, TorGuard, VyprVPN and Windscribe VPN servers using Go, OpenVPN, iptables, DNS over TLS, ShadowSocks and an HTTP proxy",
            "categories": ["VPN"],
            "network": "proxy",
            "privileged": true,
            "restart_policy": "unless-stopped",
            "ports": ["8888:8888/tcp","8388:8388/tcp","8388:8388/udp","8000:8000/tcp"],
            "env": [
                {
                    "default": "1000",
                    "label": "PUID",
                    "name": "PUID"
                },
                {
                    "default": "1000",
                    "label": "PGID",
                    "name": "PGID"
                },
                {
                    "default": "America/Chicago",
                    "label": "Time Zone",
                    "name": "TZ"
                },
                {
                    "default": "private internet access",
                    "label": "VPN Service Provider",
                    "name": "VPNSP",
                    "select": [
                        {
                            "text": "cyberghost",
                            "value": "cyberghost"
                        },
                        {
                            "text": "fastestvpn",
                            "value": "fastestvpn"
                        },
                        {
                            "text": "hidemyass",
                            "value": "hidemyass"
                        },
                        {
                            "text": "mullvad",
                            "value": "mullvad"
                        },
                        {
                            "text": "nordvpn",
                            "value": "nordvpn"
                        },
                        {
                            "text": "privado",
                            "value": "privado"
                        },
                        {
                            "text": "pia",
                            "value": "pia"
                        },
                        {
                            "text": "private internet access",
                            "value": "private internet access",
                            "default": true
                        },
                        {
                            "text": "privatevpn",
                            "value": "privatevpn"
                        },
                        {
                            "text": "protonvpn",
                            "value": "protonvpn"
                        },
                        {
                            "text": "purevpn",
                            "value": "purevpn"
                        },
                        {
                            "text": "surfshark",
                            "value": "surfshark"
                        },
                        {
                            "text": "torguard",
                            "value": "torguard"
                        },
                        {
                            "text": "vyprvpn",
                            "value":"vyprvpn"
                        },
                        {
                            "text": "windscribe",
                            "value": "windscribe"
                        }
                    ]
                },
                {
                    "default": "US Houston",
                    "label": "Region",
                    "name": "REGION"
                },
                {
                    "default": "CHANGEME",
                    "label": "VPN Username",
                    "name": "OPENVPN_USER"
                },
                {
                    "default": "CHANGEME",
                    "label": "VPN Password",
                    "name": "OPENVPN_PASSWORD"
                }
            ],
            "volumes": [
                {
                    "bind": "/opt/volumes/config/gluten/",
                    "container": "/config"
                }
            ]
        },
        {
            "type": 1,
            "title": "Jackett",
            "name": "jackett",
            "platform": "linux",
            "image": "ghcr.io/linuxserver/jackett",
            "logo": "https://raw.githubusercontent.com/thelowtech/HomeLab/master/Images/jackett.png",
            "description": "Jackett works as a proxy server: it translates queries from apps (Sonarr, Radarr, SickRage, CouchPotato, Mylar, Lidarr, DuckieTV, qBittorrent, Nefarious etc.) into tracker-site-specific http queries, parses the html response, then sends results back to the requesting software. This allows for getting recent uploads (like RSS) and performing searches. Jackett is a single repository of maintained indexer scraping & translation logic - removing the burden from other apps.",
            "categories": ["Downloaders, Tools"],
            "network": "proxy",
            "restart_policy": "unless-stopped",
            "ports": ["9117:9117/tcp"],
            "env": [
                {
                    "default": true,
                    "label": "Auto Update",
                    "name": "AUTO_UPDATE"
                },
                {
                    "default": "",
                    "label": "Additional Run Options",
                    "name": "RUN_OPTS"
                },
                {
                    "default": "America/Chicago",
                    "label": "Time Zone",
                    "name": "TZ"
                },
                {
                    "default": "1000",
                    "label": "PUID",
                    "name": "PUID"
                },
                {
                    "default": "1000",
                    "label": "PGID",
                    "name": "PGID"
                }
            ],
            "volumes": [
                {
                    "bind": "/opt/volumes/configs/jackett",
                    "container": "/config"
                },
                {
                    "bind": "/opt/volumes/storage/downloads/blackhole",
                    "container": "/downloads"
                }
            ]
        },
