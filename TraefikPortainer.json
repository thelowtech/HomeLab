                    "default": "America/Chicago",
                    "label": "Time Zone",
                    "name": "TZ"
                },
                {
                    "default": "1000",
                    "label": "PUID",
                    "name": "PUID"
                },
                {
                    "default": "1000",
                    "label": "PGID",
                    "name": "PGID"
                }
            ],
            "volumes": [
                {
                    "bind": "/opt/volumes/configs/jackett",
                    "container": "/config"
