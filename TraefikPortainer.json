                    "label": "REGION",
                    "name": "REGION"
                },
                {
                    "default": "CHANGEME",
                    "label": "OPENVPN_USER",
                    "name": "OPENVPN_USER"
                },
                {
                    "default": "CHANGEME",
                    "label": "OPENVPN_PASSWORD",
                    "name": "OPENVPN_PASSWORD"
                },
                {
                    "select": [
                        {
                            "text": "cyberghost",
                            "value": "cyberghost"
                        },
                        {
                            "text": "fastestvpn",
                            "value": "fastestvpn"
                        },
                        {
                            "text": "hidemyass",
                            "value": "hidemyass"
                        },
                        {
                            "text": "mullvad",
                            "value": "mullvad"
                        },
                        {
                            "text": "nordvpn",
                            "value": "nordvpn"
                        },
                        {
                            "text": "privado",
                            "value": "privado"
                        },
                        {
                            "text": "pia",
                            "value": "pia"
                        },
                        {
                            "text": "private internet access",
                            "value": "private internet access",
                            "default": true
                        },
                        {
                            "text": "privatevpn",
                            "value": "privatevpn"
                        },
                        {
                            "text": "protonvpn",
                            "value": "protonvpn"
                        },
                        {
                            "text": "purevpn",
                            "value": "purevpn"
                        },
                        {
                            "text": "surfshark",
                            "value": "surfshark"
                        },
                        {
                            "text": "torguard",
                            "value": "torguard"
                        },
                        {
                            "text": "vyprvpn",
                            "value":"vyprvpn"
                        },
                        {
                            "text": "windscribe",
                            "value": "windscribe"
                        }
                    ]
                }
            ]
        }
    ]
