{
    "version": "2",
    "templates": [
        {
            "categories": [
                "VPN"
            ],
            "description": "Lightweight swiss-knife-like VPN client to tunnel to Cyberghost, FastestVPN, HideMyAss, Mullvad, NordVPN, Privado, Private Internet Access, PrivateVPN, ProtonVPN, PureVPN, Surfshark, TorGuard, VyprVPN and Windscribe VPN servers using Go, OpenVPN, iptables, DNS over TLS, ShadowSocks and an HTTP proxy",
            "env": [
                {
                    "default": "PUID",
                    "label": "1000",
                    "name": "1000"
                },
                {
                    "default": "PGID",
                    "label": "1000",
                    "name": "1000"
                },
                {
                    "default": "America/Chicago",
                    "label": "TZ",
                    "name": "TZ"
                },
                {
                    "default": "private internet access",
                    "label": "VPNSP",
                    "name": "VPNSP",
                    "select": [
                        {
                            "text": "cyberghost",
                            "value": "cyberghost"
                        },
                        {
                            "text": "fastestvpn",
                            "value": "fastestvpn"
                        },
                        {
                            "text": "hidemyass",
                            "value": "hidemyass"
                        },
                        {
                            "text": "mullvad",
                            "value": "mullvad"
                        },
                        {
                            "text": "nordvpn",
                            "value": "nordvpn"
                        },
                        {
                            "text": "privado",
                            "value": "privado"
                        },
                        {
                            "text": "pia",
                            "value": "pia"
                        },
                        {
                            "text": "private internet access",
                            "value": "private internet access",
                            "default": true
                        },
                        {
                            "text": "privatevpn",
                            "value": "privatevpn"
                        },
                        {
                            "text": "protonvpn",
                            "value": "protonvpn"
                        },
                        {
                            "text": "purevpn",
                            "value": "purevpn"
                        },
                        {
                            "text": "surfshark",
                            "value": "surfshark"
                        },
                        {
                            "text": "torguard",
                            "value": "torguard"
                        },
                        {
                            "text": "vyprvpn",
                            "value":"vyprvpn"
                        },
                        {
                            "text": "windscribe",
                            "value": "windscribe"
                        }
                    ]
                },
                {
                    "default": "REGION",
                    "label": "US Houston",
                    "name": "US Houston"
                },
                {
                    "default": "CHANGEME",
                    "label": "OPENVPN_USER",
                    "name": "OPENVPN_USER"
                },
                {
                    "default": "CHANGEME",
                    "label": "OPENVPN_PASSWORD",
                    "name": "OPENVPN_PASSWORD"
                }
            ],
            "image": "qmcgaw/gluetun",
            "logo": "https://raw.githubusercontent.com/SelfhostedPro/selfhosted_templates/master/Images/airsonic-logo.png",
            "name": "gluetun",
            "platform": "linux",
            "restart_policy": "unless-stopped",
            "title": "Gluetun",
            "type": 1,
            "network": "proxy",
            "cap_add": [
                "NET_ADMIN"
            ],
            "volumes": [
                {
                    "bind": "/opt/volumes/config/gluten/",
                    "container": "/config"
                }
            ]
        }
    ]
}
