{
    "version": "2",
    "templates": [
        {
            "type": 1,
            "title": "Gluetun",
            "name": "gluetun",
            "platform": "linux",
            "image": "qmcgaw/gluetun",
            "logo": "https://d36jcksde1wxzq.cloudfront.net/be7833db9bddb4494d2a7c3dd659199a.png",
            "description": "Lightweight swiss-knife-like VPN client to tunnel to Cyberghost, FastestVPN, HideMyAss, Mullvad, NordVPN, Privado, Private Internet Access, PrivateVPN, ProtonVPN, PureVPN, Surfshark, TorGuard, VyprVPN and Windscribe VPN servers using Go, OpenVPN, iptables, DNS over TLS, ShadowSocks and an HTTP proxy",
            "categories": ["VPN"],
            "network": "proxy",
            "privileged": true,
            "restart_policy": "unless-stopped",
            "ports": [
				"8888:8888/tcp",
                "8388:8388/tcp",
                "8388:8388/udp",
                "8000:8000/tcp"
			],
            "env": [
                {
                    "default": "1000",
                    "label": "PUID",
                    "name": "PUID"
                },
                {
                    "default": "1000",
                    "label": "PGID",
                    "name": "PGID"
                },
                {
                    "default": "America/Chicago",
                    "label": "TZ",
                    "name": "TZ"
                },
                {
                    "default": "private internet access",
                    "label": "VPNSP",
                    "name": "VPNSP",
                    "select": [
                        {
                            "text": "cyberghost",
                            "value": "cyberghost"
                        },
                        {
                            "text": "fastestvpn",
                            "value": "fastestvpn"
                        },
                        {
                            "text": "hidemyass",
                            "value": "hidemyass"
                        },
                        {
                            "text": "mullvad",
                            "value": "mullvad"
                        },
                        {
                            "text": "nordvpn",
                            "value": "nordvpn"
                        },
                        {
                            "text": "privado",
                            "value": "privado"
                        },
                        {
                            "text": "pia",
                            "value": "pia"
                        },
                        {
                            "text": "private internet access",
                            "value": "private internet access",
                            "default": true
                        },
                        {
                            "text": "privatevpn",
                            "value": "privatevpn"
                        },
                        {
                            "text": "protonvpn",
                            "value": "protonvpn"
                        },
                        {
                            "text": "purevpn",
                            "value": "purevpn"
                        },
                        {
                            "text": "surfshark",
                            "value": "surfshark"
                        },
                        {
                            "text": "torguard",
                            "value": "torguard"
                        },
                        {
                            "text": "vyprvpn",
                            "value":"vyprvpn"
                        },
                        {
                            "text": "windscribe",
                            "value": "windscribe"
                        }
                    ]
                },
                {
                    "default": "US Houston",
                    "label": "REGION",
                    "name": "REGION"
                },
                {
                    "default": "CHANGEME",
                    "label": "OPENVPN_USER",
                    "name": "OPENVPN_USER"
                },
                {
                    "default": "CHANGEME",
                    "label": "OPENVPN_PASSWORD",
                    "name": "OPENVPN_PASSWORD"
                }
            ],
            "volumes": [
                {
                    "bind": "/opt/volumes3/config/gluten/",
                    "container": "/config"
                }
            ]
        },
        {
            "type": 1,
            "title": "qBittorrent",
            "name": "qbluetun",
            "platform": "linux",
            "image": "ghcr.io/linuxserver/qbittorrent",
            "logo": "https://d36jcksde1wxzq.cloudfront.net/be7833db9bddb4494d2a7c3dd659199a.png",
            "description": "The qBittorrent project aims to provide an open-source software alternative to ÂµTorrent.",
            "categories": ["Downloaders"],
            "network": "proxy",
            "restart_policy": "unless-stopped",
            "ports": [
                "6881:6881",
                "6881:6881/udp",
                "8080:8080"
            ],
            "env": [
                {
                    "default": "8080",
                    "label": "WEBUI_PORT",
                    "name": "WEBUI_PORT"
                },
                {
                    "default": "America/Chicago",
                    "label": "TZ",
                    "name": "TZ"
                },
                {
                    "default": "1000",
                    "label": "PUID",
                    "name": "PUID"
                },
                {
                    "default": "1000",
                    "label": "PGID",
                    "name": "PGID"
                }
            ],
            "volumes": [
                {
                    "bind": "/opt/volumes3/configs/qbittorrent",
                    "container": "/config"
                },
                {
                    "bind": "/opt/volumes3/storage/downloads",
                    "container": "/downloads"
                }
            ]
        },
        {
            "type": 1,
            "title": "Jackett",
            "name": "jackett",
            "platform": "linux",
            "image": "ghcr.io/linuxserver/jackett",
            "logo": "https://d36jcksde1wxzq.cloudfront.net/be7833db9bddb4494d2a7c3dd659199a.png",
            "description": "Jackett works as a proxy server: it translates queries from apps (Sonarr, Radarr, SickRage, CouchPotato, Mylar, Lidarr, DuckieTV, qBittorrent, Nefarious etc.) into tracker-site-specific http queries, parses the html response, then sends results back to the requesting software. This allows for getting recent uploads (like RSS) and performing searches. Jackett is a single repository of maintained indexer scraping & translation logic - removing the burden from other apps.",
            "categories": ["Downloaders, Tools"],
            "network": "proxy",
            "restart_policy": "unless-stopped",
            "ports": [
                "9117:9117"
            ],
            "env": [
                {
                    "default": true,
                    "label": "AUTO_UPDATE",
                    "name": "AUTO_UPDATE"
                },
                {
                    "default": "",
                    "label": "RUN_OPTS",
                    "name": "RUN_OPTS"
                },
                {
                    "default": "America/Chicago",
                    "label": "TZ",
                    "name": "TZ"
                },
                {
                    "default": "1000",
                    "label": "PUID",
                    "name": "PUID"
                },
                {
                    "default": "1000",
                    "label": "PGID",
                    "name": "PGID"
                }
            ],
            "volumes": [
                {
                    "bind": "/opt/volumes3/configs/jackett",
                    "container": "/config"
                },
                {
                    "bind": "/opt/volumes3/storage/downloads/blackhole",
                    "container": "/downloads"
                }
            ]
        },
        {
            "type": 3,
            "title": "Gluetun, qBittorrent, Jacket with Traefik",
            "description": "Deploys the Gluten Container. The qBittorrent and Jacket attach thir networks to the Gluden Container to use it as a VPN proxy. Designed to work with Traefik Proxy.",
            "note": "Requires an update to the Traefik config.yml see example on github https://github.com/thelowtech/HomeLab",
            "categories": ["VPN", "Downloaders"],
            "platform": "linux",
            "logo": "https://d36jcksde1wxzq.cloudfront.net/be7833db9bddb4494d2a7c3dd659199a.png",
            "repository": {
              "url": "https://github.com/thelowtech/HomeLab",
              "stackfile": "TraefikPortainer/Stacks/Gluetun-qBittorrent-Jacket.yml"
            },
            "env": [
                {
                    "default": "1000",
                    "label": "PUID",
                    "name": "PUID"
                },
                {
                    "default": "1000",
                    "label": "PGID",
                    "name": "PGID"
                },
                {
                    "default": "America/Chicago",
                    "label": "TZ",
                    "name": "TZ"
                },
                {
                    "default": "US Houston",
                    "label": "REGION",
                    "name": "REGION"
                },
                {
                    "default": "CHANGEME",
                    "label": "OPENVPN_USER",
                    "name": "OPENVPN_USER"
                },
                {
                    "default": "CHANGEME",
                    "label": "OPENVPN_PASSWORD",
                    "name": "OPENVPN_PASSWORD"
                },
                {
                    "default": "private internet access",
                    "label": "VPNSP",
                    "name": "VPNSP",
                    "select": [
                        {
                            "text": "cyberghost",
                            "value": "cyberghost"
                        },
                        {
                            "text": "fastestvpn",
                            "value": "fastestvpn"
                        },
                        {
                            "text": "hidemyass",
                            "value": "hidemyass"
                        },
                        {
                            "text": "mullvad",
                            "value": "mullvad"
                        },
                        {
                            "text": "nordvpn",
                            "value": "nordvpn"
                        },
                        {
                            "text": "privado",
                            "value": "privado"
                        },
                        {
                            "text": "pia",
                            "value": "pia"
                        },
                        {
                            "text": "private internet access",
                            "value": "private internet access",
                            "default": true
                        },
                        {
                            "text": "privatevpn",
                            "value": "privatevpn"
                        },
                        {
                            "text": "protonvpn",
                            "value": "protonvpn"
                        },
                        {
                            "text": "purevpn",
                            "value": "purevpn"
                        },
                        {
                            "text": "surfshark",
                            "value": "surfshark"
                        },
                        {
                            "text": "torguard",
                            "value": "torguard"
                        },
                        {
                            "text": "vyprvpn",
                            "value":"vyprvpn"
                        },
                        {
                            "text": "windscribe",
                            "value": "windscribe"
                        }
                    ]
                }
            ]
        }
    ]
}